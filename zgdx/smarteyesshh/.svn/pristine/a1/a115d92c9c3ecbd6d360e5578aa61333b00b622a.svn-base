package cn.com.ctbri.smarteyesshh.dataextract;


/**
 * @author zeromem
 * @date 2017/11/20
 */
public class Summary {
    public Long duration;
    public Long msgCounter;
    public Long workCallCounter;
    public Long restCallCounter;
    public Long under15SecCounter;
    public Long above3MinCounter;

    public static Summary createNew() {
        Summary summary = new Summary();
        summary.duration = 0L;
        summary.msgCounter = 0L;
        summary.workCallCounter = 0L;
        summary.restCallCounter = 0L;
        summary.under15SecCounter = 0L;
        summary.above3MinCounter = 0L;
        return summary;
    }


    @Override
    public String toString() {
        return "Summary{" +
                "duration=" + duration +
                ", msgCounter=" + msgCounter +
                ", workCallCounter=" + workCallCounter +
                ", restCallCounter=" + restCallCounter +
                ", under15SecCounter=" + under15SecCounter +
                ", above3MinCounter=" + above3MinCounter +
                '}';
    }
}
